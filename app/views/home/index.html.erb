<div class="row">
  <div class="small-8 small-centered columns feed-wrapper">
    
    <h1>home page</h1>

    <div class="user-feed">
      <% @users.each do |user| %>
        <div class="row feed-profile">
          <div class="small-3 columns">
            <%= link_to (image_tag user.photo.url(:small)), user_path(id: user.id) %>
          </div>
          <div class="small-9 columns">
            <p>name: <%= user.firstname %> <%= user.lastname %></p>
            <p>gender: <%= user.gender %></p>
            <p>age: <%= user.age %></p>
            <p>about me: <%= user.description %></p>
          </div>
        </div>
        <br />
        <br />
      <% end %>
    </div>

  </div>
</div>

